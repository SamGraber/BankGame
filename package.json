{
  "name": "bank-game",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run database\" \"npm run tsc:w\" \"npm run server\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
	"database": "mongod --dbpath ./data",
    "lite": "lite-server",
    "http": "http-server",
    "server": "node ./server/run.js",
    "typings": "typings",
    "postinstall": "jspm install && typings install"
  },
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.0",
    "cookie-parser": "^1.4.1",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "mongodb": "^1.4.4",
    "monk": "^1.0.1",
    "morgan": "^1.7.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "jspm": "^0.16.31"
  },
  "jspm": {
    "dependencies": {
      "angular2": "npm:angular2@^2.0.0-beta.12",
      "es6-promise": "npm:es6-promise@^3.1.2",
      "es6-shim": "npm:es6-shim@^0.35.0",
      "reflect-metadata": "npm:reflect-metadata@^0.1.3",
      "rxjs": "npm:rxjs@^5.0.0-beta.3",
      "zone.js": "npm:zone.js@^0.6.6"
    },
    "devDependencies": {
      "concurrently": "npm:concurrently@^2.0.0",
      "lite-server": "npm:lite-server@^2.1.0",
      "typescript": "npm:typescript@^1.6.2",
      "typings": "npm:typings@^0.7.9"
    }
  }
