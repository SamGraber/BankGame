{
  "name": "bank-game",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run database\" \"npm run build.watch\" \"npm run server\" ",
    "build": "tsc",
    "build.watch": "tsc -w",
    "build-server": "cd ./server && tsc",
    "build-server.watch": "cd ./server && tsc -w",
    "database": "mongod --dbpath ./data",
    "lite": "lite-server",
    "http": "http-server",
    "server": "node ./server/run.js",
    "typings": "typings install && cd ./server && typings install",
    "postinstall": "npm run typings"
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "^2.0.0-beta.12",
    "body-parser": "^1.15.0",
    "cookie-parser": "^1.4.1",
    "ejs": "^2.4.1",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "express": "^4.13.4",
    "jquery": "^2.2.3",
    "lodash": "^4.11.1",
    "mongodb": "^1.4.4",
    "monk": "^1.0.1",
    "morgan": "^1.7.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.3",
    "serve-favicon": "^2.3.0",
    "systemjs": "0.19.25",
    "toastr": "^2.1.2",
    "zone.js": "^0.6.6"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.1.0",
    "typescript": "^1.6.2",
    "typings": "^0.7.9"
  }
}
